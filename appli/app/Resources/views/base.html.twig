<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-16" />
        <meta name="author" content="Simon Vivier">
        <title>{% block title %}Zenity{% endblock %}</title>
        {% block stylesheets %}
            <link rel="stylesheet" href="{{ asset('vendor/bootstrap/css/bootstrap.min.css') }}" media="screen">
            <link href="{{ asset('css/print.css') }}" rel="stylesheet" media="print" />
            <link rel="stylesheet" href="{{ asset('vendor/bootstrap/css/bootstrap-theme.min.css') }}">
            <link rel="stylesheet" href="{{ asset('vendor/daterangepicker/daterangepicker.css') }}">
            <link rel="stylesheet" href="{{ asset('vendor/jquery-ui/jquery-ui.min.css') }}">
            <link rel="stylesheet" href="{{ asset('vendor/jquery-ui/jquery-ui.theme.min.css') }}">
            <link rel="stylesheet" href="{{ asset('vendor/c3/c3.min.css') }}">
            <link rel="stylesheet" href="{{ asset('css/navbar-zenity.css') }}">
            <link rel="stylesheet" href="{{ asset('css/login.css') }}">

        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('ressources/favicon.ico') }}"/>
    </head>
    <body>
    {% block menu %}
    {% endblock %}
    <span id="body">
        {% block body %}
        {% endblock %}
    </span>
    {% block javascripts %}
        <script src="{{ asset('config.js') }}"></script>
        <script src="{{ asset('vendor/hashFunction.min.js') }}"></script>
        <script src="{{ asset('vendor/jquery.min.js') }}"></script>
        <script src="{{ asset('vendor/jquery-ui/jquery-ui.min.js') }}"></script>
        <script src="{{ asset('vendor/bootstrap/js/bootstrap.min.js') }}"></script>
        <script src="{{ asset('vendor/js.cookie.js') }}"></script>
        <script src="{{ asset('vendor/handlebars.js') }}"></script>
        <script src="{{ asset('js/security.js') }}"></script>
        <script src="{{ asset('vendor/spin.min.js') }}"></script>
        <script src="{{ asset('vendor/jquery.printElement.min.js') }}"></script>
        <script src="{{ asset('js/secret.js') }}"></script>

        <script>
            logged();
        </script>
    {% endblock %}
    </body>
</html>
